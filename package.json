{
  "name": "skedify-sdk",
  "version": "0.0.0-development",
  "description": "JavaScript SDK that helps integrators to interact with the Skedify API",
  "main": "lib/skedify-sdk.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/skedify/sdk-js.git"
  },
  "author": {
    "name": "Skedify",
    "email": "info@skedify.co",
    "url": "https://www.skedify.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skedify/sdk-js/issues"
  },
  "homepage": "https://github.com/skedify/sdk-js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rollup -c rollup.config.prod.js",
    "watch": "rollup -c rollup.config.dev.js --watch",
    "test": "cross-env NODE_ENV=test jest",
    "tdd": "npm run test -- --watch",
    "docs": "serve",
    "reload": "livereload lib/",
    "start": "npm-run-all --parallel watch reload docs",
    "lint": "eslint src",
    "format": "npm run lint -- --fix",
    "check-coverage": "cross-env CI=true NODE_ENV=test jest --coverage",
    "precommit": "npm run lint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepush": "npm run check-coverage",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-polyfills": "^1.8.0",
    "babel-preset-env": "^1.7.0",
    "commitizen": "^2.10.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-require-in-package": "^1.0.3",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "livereload": "^0.7.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-inject": "^2.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.3.1",
    "semantic-release": "^15.9.1",
    "serve": "^9.4.0",
    "travis-deploy-once": "^5.0.1",
    "uglify-es": "^3.3.10"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "moxios": "^0.4.0",
    "uuid": "^3.3.2"
  }
}
