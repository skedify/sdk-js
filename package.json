{
  "name": "skedify-sdk",
  "version": "0.0.0-development",
  "description": "Javascript SDK that helps integrators to interact with the Skedify API",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/skedify/sdk-js.git"
  },
  "author": {
    "name": "Skedify",
    "email": "info@skedify.co",
    "url": "https://www.skedify.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skedify/sdk-js/issues"
  },
  "homepage": "https://github.com/skedify/sdk-js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rollup -c rollup.config.prod.js",
    "watch": "rollup -c rollup.config.dev.js --watch",
    "test": "NODE_ENV=test jest",
    "tdd": "npm run test -- --watch",
    "lint": "eslint src",
    "format": "npm run lint -- --fix",
    "check-coverage": "CI=true NODE_ENV=test jest --coverage",
    "publish-coverage": "codecov",
    "precommit": "npm run lint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepush": "npm test && npm run check-coverage",
    "prepublish": "npm run build",
    "postmerge": "npm install",
    "postrewrite": "npm install",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^5.2.5",
    "@commitlint/config-conventional": "^5.2.3",
    "babel-eslint": "^8.0.3",
    "codecov": "^3.0.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-better": "^0.1.5",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-require-in-package": "^1.0.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-watch": "^4.3.1",
    "semantic-release": "^8.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
